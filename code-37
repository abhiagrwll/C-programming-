//palindrome
#include <stdio.h>
#include <string.h>
int main()
{
    char st[100];
    int i,flag=0,j;
    scanf("%[^\n]",st);
    for(i=0,j=strlen(st)-1;i<=j;i++,j--)
        if(st[i]!=st[j])
            flag=1;
        if(flag==1)
            printf("string is not palindrome");
        else
            printf("string is palindrome");
        return 0;
}
